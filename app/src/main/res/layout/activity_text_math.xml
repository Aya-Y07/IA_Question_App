<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="mx.edu.greengates.ia_question_app.data.Texts_Physics">


    <Button
        android:id="@+id/question_trignometry"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/questions_trignometry"
        app:layout_constraintBottom_toTopOf="@+id/questions_algebra"
        app:layout_constraintEnd_toStartOf="@+id/solutions_trignometry"
        app:layout_constraintHorizontal_bias="0.893"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="DuplicateSpeakableTextCheck" />

    <Button
        android:id="@+id/solutions_trignometry"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/solutions"
        app:layout_constraintBottom_toTopOf="@+id/solutions_algebra"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.859"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="DuplicateSpeakableTextCheck" />

    <Button
        android:id="@+id/questions_algebra"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/questions_dif"
        app:layout_constraintBottom_toTopOf="@+id/questions_vector"
        app:layout_constraintEnd_toStartOf="@+id/solutions_algebra"
        app:layout_constraintHorizontal_bias="0.893"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/question_trignometry" />

    <Button
        android:id="@+id/solutions_algebra"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/solutions"
        app:layout_constraintBottom_toTopOf="@+id/solutions_vector"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.859"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/solutions_trignometry" />

    <Button
        android:id="@+id/questions_vector"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/questions_vec"
        app:layout_constraintBottom_toTopOf="@+id/questions_complex_numebr"
        app:layout_constraintEnd_toStartOf="@+id/solutions_vector"
        app:layout_constraintHorizontal_bias="0.893"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/questions_algebra" />

    <Button
        android:id="@+id/solutions_vector"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/solutions_vec"
        app:layout_constraintBottom_toTopOf="@+id/solutions_complex_numebr"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.859"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/solutions_algebra" />

    <Button
        android:id="@+id/questions_complex_numebr"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/questions_comp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/solutions_complex_numebr"
        app:layout_constraintHorizontal_bias="0.893"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/questions_vector" />

    <Button
        android:id="@+id/solutions_complex_numebr"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/solutions_comp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.859"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/solutions_vector" />

    <TextView
        android:id="@+id/text_trignometry"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/text_trig"
        app:layout_constraintBottom_toTopOf="@+id/text_akgebra"
        app:layout_constraintEnd_toStartOf="@+id/question_trignometry"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/text_akgebra"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/text_alg"
        app:layout_constraintBottom_toTopOf="@+id/text_vector"
        app:layout_constraintEnd_toStartOf="@+id/questions_algebra"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/text_trignometry" />

    <TextView
        android:id="@+id/text_vector"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/text_vec"
        app:layout_constraintBottom_toTopOf="@+id/text_complex_number"
        app:layout_constraintEnd_toStartOf="@+id/questions_vector"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/text_akgebra" />

    <TextView
        android:id="@+id/text_complex_number"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/text_comp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/questions_complex_numebr"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/text_vector" />


</androidx.constraintlayout.widget.ConstraintLayout>